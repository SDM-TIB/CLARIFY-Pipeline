<file:///mnt/e/CLARIFY-KG-pipeline/mapping/bc_clinical_notes_HUPHM/BCPatient_bc_clinical_notes_HUPHM_family_history>
    a rr:TriplesMap;
    rml:logicalSource [ rml:source "/mnt/e/CLARIFY-KG-pipeline/output/Dragoman/bc_clinical_notes_HUPHM/family_history_bc_clinical_notes_HUPHM_PROJECT1.csv";
                rml:referenceFormulation ql:CSV
                ];
    rr:subjectMap [
        rr:template "http://research.tib.eu/clarify2020/entity/BC_HUPHM_{ehr}";
        rr:class clarify:BCPatient
    ];
    rr:predicateObjectMap [
        rr:predicate clarify:hasFamilyCancerType;
        rr:objectMap [
        rml:reference "findSemantic_HUPHM_BreastCancer_ub3bL60C24";
        rr:termType rr:IRI
        ]
    ].

<file:///mnt/e/CLARIFY-KG-pipeline/mapping/bc_clinical_notes_HUPHM/FamilyCancerType_bc_clinical_notes_HUPHM_family_history>
    a rr:TriplesMap;
