<file:///mnt/e/CLARIFY-KG-pipeline/mapping/lc_clinical_notes_HUPHM/CurrentStatus_lc_clinical_notes_HUPHM_monitoring>
    a rr:TriplesMap;
    rml:logicalSource [ rml:source "/data/lc_clinical_notes_HUPHM/monitoring_lc_clinical_notes_HUPHM_PROJECT1.csv";
                rml:referenceFormulation ql:CSV
                ];
    rr:subjectMap [
        rr:template "http://research.tib.eu/clarify2020/entity/{status}";
    ];
    rr:predicateObjectMap [
        rr:predicate rdf:type;
        rr:objectMap [
        rr:template  "http://research.tib.eu/clarify2020/vocab/CurrentStatus"
        ]
    ].

<file:///mnt/e/CLARIFY-KG-pipeline/mapping/lc_clinical_notes_HUPHM/StatusEvolution_lc_clinical_notes_HUPHM_monitoring>
