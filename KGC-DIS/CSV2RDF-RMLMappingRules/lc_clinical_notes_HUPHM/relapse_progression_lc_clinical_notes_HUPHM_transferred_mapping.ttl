<file:///mnt/e/CLARIFY-KG-pipeline/mapping/lc_clinical_notes_HUPHM/LCPatient_lc_clinical_notes_HUPHM_relapse_progression>
    a rr:TriplesMap;
    rml:logicalSource [ rml:source "/data/lc_clinical_notes_HUPHM/relapse_progression_lc_clinical_notes_HUPHM_PROJECT1.csv";
                rml:referenceFormulation ql:CSV
                ];
    rr:subjectMap [
        rr:template "http://research.tib.eu/clarify2020/entity/HUPHM_{EHR}";
        rr:class clarify:LCPatient
    ];
    rr:predicateObjectMap [
        rr:predicate clarify:hasProgressionRelapseAssessment;
        rr:objectMap [
        rr:parentTriplesMap <file:///mnt/e/CLARIFY-KG-pipeline/mapping/lc_clinical_notes_HUPHM/progressionRelapseObservation_lc_clinical_notes_HUPHM_relapse_progression>;
        rr:joinCondition [
            rr:child "EHR";
            rr:parent "EHR";
        ];
        rr:joinCondition [
            rr:child "occurrence_date";
            rr:parent "occurrence_date";
        ];
        ]
    ].

