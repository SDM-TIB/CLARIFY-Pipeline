<file:///mnt/e/CLARIFY-KG-pipeline/mapping/lc_clinical_notes_HUPHM/CancerType_lc_clinical_notes_HUPHM_patient>
    a rr:TriplesMap;
    rml:logicalSource [ rml:source "/data/lc_clinical_notes_HUPHM/patient_lc_clinical_notes_HUPHM_PROJECT6.csv";
                rml:referenceFormulation ql:CSV
                ];
    rr:subjectMap [
        rml:reference "findSemantic_HUPHM_ub13bL138C24";
        rr:termType rr:IRI
    ];
    rr:predicateObjectMap [
        rr:predicate clarify:hasOriginalValue;
        rr:objectMap [
        rml:reference "type_patient"
        ]
    ];
    rr:predicateObjectMap [
        rr:predicate rdf:type;
        rr:objectMap [
        rr:template  "http://research.tib.eu/clarify2020/vocab/CancerType"
