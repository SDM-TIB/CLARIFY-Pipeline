<file:///mnt/e/CLARIFY-KG-pipeline/mapping/lc_clinical_notes_HUPHM/City_lc_clinical_notes_HUPHM_patient>
    a rr:TriplesMap;
    rml:logicalSource [ rml:source "/data/lc_clinical_notes_HUPHM/patient_lc_clinical_notes_HUPHM_PROJECT1.csv";
                rml:referenceFormulation ql:CSV
                ];
    rr:subjectMap [
        rr:template "http://clarify2020.eu/City/{cityID}";
        rr:class clarify:City
    ].

<file:///mnt/e/CLARIFY-KG-pipeline/mapping/lc_clinical_notes_HUPHM/Hospital_lc_clinical_notes_HUPHM_patient>
    a rr:TriplesMap;
    rml:logicalSource [ rml:source "/data/lc_clinical_notes_HUPHM/patient_lc_clinical_notes_HUPHM_PROJECT2.csv";
                rml:referenceFormulation ql:CSV
                ];
    rr:subjectMap [
        rr:template "http://clarify2020.eu/Hospital/{organization_id}";
        rr:class clarify:Hospital
    ].

<file:///mnt/e/CLARIFY-KG-pipeline/mapping/lc_clinical_notes_HUPHM/LCPatient_lc_clinical_notes_HUPHM_patient>
    a rr:TriplesMap;
    rml:logicalSource [ rml:source "/data/lc_clinical_notes_HUPHM/patient_lc_clinical_notes_HUPHM_PROJECT3.csv";
                rml:referenceFormulation ql:CSV
                ];
    rr:subjectMap [
        rr:template "http://research.tib.eu/clarify2020/entity/HUPHM_{EHR}";
        rr:class clarify:LCPatient
    ];
    rr:predicateObjectMap [
        rr:predicate clarify:sex;
        rr:objectMap [
        rr:parentTriplesMap <file:///mnt/e/CLARIFY-KG-pipeline/mapping/lc_clinical_notes_HUPHM/Sex_lc_clinical_notes_HUPHM_patient>;
        rr:joinCondition [
            rr:child "gender";
            rr:parent "gender";
        ];
        ]
    ];
    rr:predicateObjectMap [
        rr:predicate clarify:locatedInCity;
        rr:objectMap [
        rr:parentTriplesMap <file:///mnt/e/CLARIFY-KG-pipeline/mapping/lc_clinical_notes_HUPHM/City_lc_clinical_notes_HUPHM_patient>;
        rr:joinCondition [
            rr:child "cityID";
            rr:parent "cityID";
        ];
        ]
    ];
    rr:predicateObjectMap [
        rr:predicate clarify:hasCurrentCancerType;
        rr:objectMap [
        rml:reference "findSemantic_HUPHM_ub13bL138C24";
        rr:termType rr:IRI
        ]
    ];
    rr:predicateObjectMap [
        rr:predicate prov:has_provenance;
        rr:objectMap [
        rr:constant "http://research.tib.eu/clarify2020/entity/HUPHM"
        ]
    ];
    rr:predicateObjectMap [
        rr:predicate clarify:hasRace;
        rr:objectMap [
        rr:parentTriplesMap <file:///mnt/e/CLARIFY-KG-pipeline/mapping/lc_clinical_notes_HUPHM/Race_lc_clinical_notes_HUPHM_patient>;
        rr:joinCondition [
            rr:child "race";
            rr:parent "race";
        ];
        ]
    ];
    rr:predicateObjectMap [
        rr:predicate clarify:birthDate;
        rr:objectMap [
        rml:reference "birth_date";
        rr:datatype xsd:date;
        ]
    ];
    rr:predicateObjectMap [
        rr:predicate clarify:locatedInHospital;
        rr:objectMap [
        rr:parentTriplesMap <file:///mnt/e/CLARIFY-KG-pipeline/mapping/lc_clinical_notes_HUPHM/Hospital_lc_clinical_notes_HUPHM_patient>;
        rr:joinCondition [
            rr:child "organization_id";
            rr:parent "organization_id";
        ];
        ]
    ].

