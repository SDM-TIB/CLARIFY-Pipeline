@prefix rr: <http://www.w3.org/ns/r2rml#> .
@prefix rml: <http://semweb.mmlab.be/ns/rml#> .
@prefix ql: <http://semweb.mmlab.be/ns/ql#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix schema: <http://schema.org/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix fnml: <http://semweb.mmlab.be/ns/fnml#> .
@prefix fno: <https://w3id.org/function/ontology#> .
@prefix clarifyFun: <http://research.tib.eu/clarify2020/function/> .
@prefix prov: <http://www.w3.org/ns/prov#> .
@prefix clarify: <http://research.tib.eu/clarify2020/vocab/> .

<file:///mnt/e/CLARIFY-KG-pipeline/mapping/lc_clinical_notes_HUPHM/Diagnosis_lc_clinical_notes_HUPHM_diagnosis>
    a rr:TriplesMap;
    rml:logicalSource [ rml:source "/data/lc_clinical_notes_HUPHM/diagnosis_lc_clinical_notes_HUPHM_PROJECT1.csv";
                rml:referenceFormulation ql:CSV
                ];
    rr:subjectMap [
        rr:template "http://research.tib.eu/clarify2020/entity/HUPHM_{EHR}_DiagnosisDate_{date}";
        rr:class clarify:Diagnosis
    ];
    rr:predicateObjectMap [
        rr:predicate clarify:has_SynchronousTumor_OrNot;
        rr:objectMap [
        rml:reference "findSemantic_ub5bL89C24";
        rr:termType rr:IRI
        ]
    ];
    rr:predicateObjectMap [
        rr:predicate clarify:hasDiagnosisStage;
        rr:objectMap [
        rr:parentTriplesMap <file:///mnt/e/CLARIFY-KG-pipeline/mapping/lc_clinical_notes_HUPHM/CancerStage_lc_clinical_notes_HUPHM_diagnosis>;
        rr:joinCondition [
            rr:child "stage";
            rr:parent "stage";
        ];
        ]
    ].

<file:///mnt/e/CLARIFY-KG-pipeline/mapping/lc_clinical_notes_HUPHM/CancerStage_lc_clinical_notes_HUPHM_diagnosis>
    a rr:TriplesMap;
    rml:logicalSource [ rml:source "/data/lc_clinical_notes_HUPHM/diagnosis_lc_clinical_notes_HUPHM_PROJECT2.csv";
                rml:referenceFormulation ql:CSV
                ];
    rr:subjectMap [
        rr:template "http://research.tib.eu/clarify2020/entity/{stage}";
    ];
    rr:predicateObjectMap [
        rr:predicate rdf:type;
        rr:objectMap [
        rr:template  "http://research.tib.eu/clarify2020/vocab/CancerStage"
        ]
    ].

<file:///mnt/e/CLARIFY-KG-pipeline/mapping/lc_clinical_notes_HUPHM/LCPatient_lc_clinical_notes_HUPHM_diagnosis>
    a rr:TriplesMap;
    rml:logicalSource [ rml:source "/data/lc_clinical_notes_HUPHM/diagnosis_lc_clinical_notes_HUPHM_PROJECT3.csv";
                rml:referenceFormulation ql:CSV
                ];
    rr:subjectMap [
        rr:template "http://research.tib.eu/clarify2020/entity/HUPHM_{EHR}";
        rr:class clarify:LCPatient
    ];
    rr:predicateObjectMap [
        rr:predicate clarify:hasDiagnosis;
        rr:objectMap [
        rr:parentTriplesMap <file:///mnt/e/CLARIFY-KG-pipeline/mapping/lc_clinical_notes_HUPHM/Diagnosis_lc_clinical_notes_HUPHM_diagnosis>;
        rr:joinCondition [
            rr:child "EHR";
            rr:parent "EHR";
        ];
        rr:joinCondition [
            rr:child "date";
            rr:parent "date";
        ];
        ]
    ].

<file:///mnt/e/CLARIFY-KG-pipeline/mapping/lc_clinical_notes_HUPHM/SynchronousTumor_lc_clinical_notes_HUPHM_diagnosis>
    a rr:TriplesMap;
    rml:logicalSource [ rml:source "/data/lc_clinical_notes_HUPHM/diagnosis_lc_clinical_notes_HUPHM_PROJECT4.csv";
                rml:referenceFormulation ql:CSV
                ];
    rr:subjectMap [
        rml:reference "findSemantic_ub5bL89C24";
        rr:termType rr:IRI
    ];
    rr:predicateObjectMap [
        rr:predicate clarify:hasOriginalValue;
        rr:objectMap [
        rml:reference "syncrhonous_tumours"
        ]
    ];
    rr:predicateObjectMap [
        rr:predicate rdf:type;
        rr:objectMap [
        rr:template  "http://research.tib.eu/clarify2020/vocab/SynchronousTumor"
        ]
    ].

