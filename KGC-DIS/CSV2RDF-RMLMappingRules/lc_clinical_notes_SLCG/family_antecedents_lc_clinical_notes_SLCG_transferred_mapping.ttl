@prefix rr: <http://www.w3.org/ns/r2rml#> .
@prefix rml: <http://semweb.mmlab.be/ns/rml#> .
@prefix ql: <http://semweb.mmlab.be/ns/ql#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix schema: <http://schema.org/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix fnml: <http://semweb.mmlab.be/ns/fnml#> .
@prefix fno: <https://w3id.org/function/ontology#> .
@prefix clarifyFun: <http://research.tib.eu/clarify2020/function/> .
@prefix prov: <http://www.w3.org/ns/prov#> .
@prefix clarify: <http://research.tib.eu/clarify2020/vocab/> .

<file:///mnt/e/CLARIFY-KG-pipeline/mapping/lc_clinical_notes_SLCG/LCPatient_lc_clinical_notes_SLCG_family_antecedents>
    a rr:TriplesMap;
    rml:logicalSource [ rml:source "/data/lc_clinical_notes_SLCG/family_antecedents_lc_clinical_notes_SLCG_PROJECT1.csv";
                rml:referenceFormulation ql:CSV
                ];
    rr:subjectMap [
        rr:template "http://research.tib.eu/clarify2020/entity/{EHR}";
        rr:class clarify:LCPatient
    ];
    rr:predicateObjectMap [
        rr:predicate clarify:numberOfFamilyWithLC;
        rr:objectMap [
        rml:reference "number_family_members_lung_cancer";
        rr:datatype xsd:int;
        ]
    ];
    rr:predicateObjectMap [
        rr:predicate clarify:numberOfFamilyWithOtherCancers;
        rr:objectMap [
        rml:reference "number_of_family_members_others";
        rr:datatype xsd:int;
        ]
    ];
    rr:predicateObjectMap [
        rr:predicate clarify:has_familyWithCancer_OrNot;
        rr:objectMap [
        rml:reference "findSemantic_ub10bL68C24";
        rr:termType rr:IRI
        ]
    ].

<file:///mnt/e/CLARIFY-KG-pipeline/mapping/lc_clinical_notes_SLCG/FamilyWithCancer_lc_clinical_notes_SLCG_family_antecedents>
    a rr:TriplesMap;
    rml:logicalSource [ rml:source "/data/lc_clinical_notes_SLCG/family_antecedents_lc_clinical_notes_SLCG_PROJECT2.csv";
                rml:referenceFormulation ql:CSV
                ];
    rr:subjectMap [
        rml:reference "findSemantic_ub10bL68C24";
        rr:termType rr:IRI
    ];
    rr:predicateObjectMap [
        rr:predicate rdf:type;
        rr:objectMap [
        rr:template  "http://research.tib.eu/clarify2020/vocab/FamilyWithCancer"
        ]
    ];
    rr:predicateObjectMap [
        rr:predicate clarify:hasOriginalValue;
        rr:objectMap [
        rml:reference "has_family_cancer_antecedents"
        ]
    ].

