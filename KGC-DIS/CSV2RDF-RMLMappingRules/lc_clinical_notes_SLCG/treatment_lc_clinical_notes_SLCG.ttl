@prefix rml: <http://semweb.mmlab.be/ns/rml#> .
@prefix ql: <http://semweb.mmlab.be/ns/ql#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix schema: <http://schema.org/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix fnml: <http://semweb.mmlab.be/ns/fnml#> .
@prefix fno: <https://w3id.org/function/ontology#> .
@prefix clarifyFun: <http://research.tib.eu/clarify2020/function/> .
@prefix prov: <http://www.w3.org/ns/prov#> .
@prefix clarify: <http://research.tib.eu/clarify2020/vocab/> .

<LCPatient_lc_clinical_notes_SLCG_diagnosis>
	rml:logicalSource [ rml:source "/data/lc_clinical_notes_SLCG/surgery.csv";
						rml:referenceFormulation ql:CSV;
						];
	rr:subjectMap [
		rr:template "http://research.tib.eu/clarify2020/entity/{EHR}";
        rr:class clarify:LCPatient
	];
	rr:predicateObjectMap [
        rr:predicate clarify:hasOncologicalTreatment;
        rr:objectMap [
            rr:template "http://research.tib.eu/clarify2020/entity/{specify_surgery_class}";
        ]
    ].
