@prefix rr: <http://www.w3.org/ns/r2rml#> .
@prefix rml: <http://semweb.mmlab.be/ns/rml#> .
@prefix ql: <http://semweb.mmlab.be/ns/ql#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix schema: <http://schema.org/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix fnml: <http://semweb.mmlab.be/ns/fnml#> .
@prefix fno: <https://w3id.org/function/ontology#> .
@prefix clarifyFun: <http://clarify2020.eu/function/> .
@prefix prov: <http://www.w3.org/ns/prov#> .
@prefix clarify: <http://research.tib.eu/clarify2020/vocab/> .

<file:///mnt/e/CLARIFY-KG-pipeline/mapping/lc_clinical_notes_SLCG/Annotation_lc_clinical_notes_SLCG_radiotherapy_drugs>
    a rr:TriplesMap;
    rml:logicalSource [ rml:source "/data/lc_clinical_notes_SLCG/radiotherapy_drugs_lc_clinical_notes_SLCG_PROJECT1.csv";
                rml:referenceFormulation ql:CSV
                ];
    rr:subjectMap [
        rr:template "http://research.tib.eu/clarify2020/entity/{CUI}";
        rr:class clarify:Annotation
    ];
    rr:predicateObjectMap [
        rr:predicate clarify:annID;
        rr:objectMap [
        rml:reference "CUI"
        ]
    ];
    rr:predicateObjectMap [
        rr:predicate prov:has_provenance;
        rr:objectMap [
        rr:template  "http://research.tib.eu/clarify2020/entity/UMLS"
        ]
    ].

<file:///mnt/e/CLARIFY-KG-pipeline/mapping/lc_clinical_notes_SLCG/RadiotherapyTreatmentLine_lc_clinical_notes_SLCG_radiotherapy_drugs>
    a rr:TriplesMap;
    rml:logicalSource [ rml:source "/data/lc_clinical_notes_SLCG/radiotherapy_drugs_lc_clinical_notes_SLCG_PROJECT2.csv";
                rml:referenceFormulation ql:CSV
                ];
    rr:subjectMap [
        rr:template "http://research.tib.eu/clarify2020/entity/{EHR}_Radiotherapy_{treatment_line}";
        rr:class clarify:Radiotherapy
    ];
    rr:predicateObjectMap [
        rr:predicate clarify:treatmentLineDosage;
        rr:objectMap [
        rml:reference "dose"
        ]
    ];
    rr:predicateObjectMap [
        rr:predicate clarify:hasCUIAnnotation;
        rr:objectMap [
        rr:parentTriplesMap <file:///mnt/e/CLARIFY-KG-pipeline/mapping/lc_clinical_notes_SLCG/Annotation_lc_clinical_notes_SLCG_radiotherapy_drugs>;
        rr:joinCondition [
            rr:child "CUI";
            rr:parent "CUI";
        ];
        ]
    ];
    rr:predicateObjectMap [
        rr:predicate clarify:treatmentLineFractioning;
        rr:objectMap [
        rml:reference "fractioning"
        ]
    ].

