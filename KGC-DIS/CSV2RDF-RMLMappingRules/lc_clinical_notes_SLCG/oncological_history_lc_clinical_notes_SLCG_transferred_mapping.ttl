<file:///mnt/e/CLARIFY-KG-pipeline/mapping/lc_clinical_notes_SLCG/previous_cancer_type_lc_clinical_notes_SLCG_oncological_history>
    a rr:TriplesMap;
    rml:logicalSource [ rml:source "/data/lc_clinical_notes_SLCG/oncological_history_lc_clinical_notes_SLCG_PROJECT1.csv";
                rml:referenceFormulation ql:CSV
                ];
    rr:subjectMap [
        rml:reference "findSemantic_ub19bL123C24";
        rr:termType rr:IRI
    ];
    rr:predicateObjectMap [
        rr:predicate rdf:type;
        rr:objectMap [
        rr:template  "http://research.tib.eu/clarify2020/vocab/CancerType"
        ]
    ];
    rr:predicateObjectMap [
        rr:predicate clarify:hasOriginalValue;
        rr:objectMap [
        rml:reference "previous_cancer_type"
        ]
    ].

<file:///mnt/e/CLARIFY-KG-pipeline/mapping/lc_clinical_notes_SLCG/LCPatient_lc_clinical_notes_SLCG_oncological_history>
