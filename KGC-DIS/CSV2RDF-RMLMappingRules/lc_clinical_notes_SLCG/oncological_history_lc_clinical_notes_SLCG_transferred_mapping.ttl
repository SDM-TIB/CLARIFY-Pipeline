                rml:referenceFormulation ql:CSV
                ];
    rr:subjectMap [
        rr:template "http://research.tib.eu/clarify2020/entity/{EHR}";
        rr:class clarify:LCPatient
    ];
    rr:predicateObjectMap [
        rr:predicate clarify:hasPreviousCancerType;
        rr:objectMap [
        rml:reference "findSemantic_ub19bL123C24";
        rr:termType rr:IRI
        ]
    ];
    rr:predicateObjectMap [
        rr:predicate clarify:has_previousCancerHistory_OrNot;
        rr:objectMap [
        rml:reference "findSemantic_ub19bL86C24";
        rr:termType rr:IRI
        ]
    ];
    rr:predicateObjectMap [
        rr:predicate clarify:previousCancerTypeDescription;
        rr:objectMap [
        rml:reference "previous_cancer_description"
        ]
    ].

