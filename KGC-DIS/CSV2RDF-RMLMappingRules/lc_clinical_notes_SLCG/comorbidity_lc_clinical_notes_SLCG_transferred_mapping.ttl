@prefix rr: <http://www.w3.org/ns/r2rml#> .
@prefix rml: <http://semweb.mmlab.be/ns/rml#> .
@prefix ql: <http://semweb.mmlab.be/ns/ql#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix schema: <http://schema.org/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix fnml: <http://semweb.mmlab.be/ns/fnml#> .
@prefix fno: <https://w3id.org/function/ontology#> .
@prefix clarifyFun: <http://clarify2020.eu/function/> .
@prefix prov: <http://www.w3.org/ns/prov#> .
@prefix clarify: <http://research.tib.eu/clarify2020/vocab/> .

<file:///mnt/e/CLARIFY-KG-pipeline/mapping/lc_clinical_notes_SLCG/LCPatient_lc_clinical_notes_SLCG_comorbidity>
    a rr:TriplesMap;
    rml:logicalSource [ rml:source "/data/lc_clinical_notes_SLCG/comorbidity_lc_clinical_notes_SLCG_PROJECT1.csv";
                rml:referenceFormulation ql:CSV
                ];
    rr:subjectMap [
        rr:template "http://research.tib.eu/clarify2020/entity/{EHR}";
        rr:class clarify:LCPatient
    ];
    rr:predicateObjectMap [
        rr:predicate clarify:hasComorbidityStatus;
        rr:objectMap [
        rml:reference "findSemantic_ub6bL75C24";
        rr:termType rr:IRI
        ]
    ];
    rr:predicateObjectMap [
        rr:predicate clarify:hasComorbidity;
        rr:objectMap [
        rr:parentTriplesMap <file:///mnt/e/CLARIFY-KG-pipeline/mapping/lc_clinical_notes_SLCG/Disorder_lc_clinical_notes_SLCG_comorbidity>;
        rr:joinCondition [
            rr:child "name";
            rr:parent "name";
        ];
        ]
    ].

<file:///mnt/e/CLARIFY-KG-pipeline/mapping/lc_clinical_notes_SLCG/Disorder_lc_clinical_notes_SLCG_comorbidity>
    a rr:TriplesMap;
    rml:logicalSource [ rml:source "/data/lc_clinical_notes_SLCG/comorbidity_lc_clinical_notes_SLCG_PROJECT2.csv";
                rml:referenceFormulation ql:CSV
                ];
    rr:subjectMap [
        rr:template "http://research.tib.eu/clarify2020/entity/{name}";
        rr:class clarify:Disorder
    ];
    rr:predicateObjectMap [
        rr:predicate clarify:disorderLabel;
        rr:objectMap [
        rml:reference "name"
        ]
    ].

<file:///mnt/e/CLARIFY-KG-pipeline/mapping/lc_clinical_notes_SLCG/positive_negative_lc_clinical_notes_SLCG_comorbidity>
    a rr:TriplesMap;
    rml:logicalSource [ rml:source "/data/lc_clinical_notes_SLCG/comorbidity_lc_clinical_notes_SLCG_PROJECT3.csv";
                rml:referenceFormulation ql:CSV
                ];
    rr:subjectMap [
        rml:reference "findSemantic_ub6bL75C24";
        rr:termType rr:IRI
    ];
    rr:predicateObjectMap [
        rr:predicate clarify:hasOriginalValue;
        rr:objectMap [
        rml:reference "positive_negative"
        ]
    ];
    rr:predicateObjectMap [
        rr:predicate rdf:type;
        rr:objectMap [
        rr:template  "http://research.tib.eu/clarify2020/vocab/ComorbidityStatus"
        ]
    ].

