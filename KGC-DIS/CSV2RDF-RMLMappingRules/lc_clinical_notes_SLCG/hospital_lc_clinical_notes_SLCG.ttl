<Hospital_lc_clinical_notes_SLCG_hospital>
	rml:logicalSource [ rml:source "/data/lc_clinical_notes_SLCG/hospital.csv";
						rml:referenceFormulation ql:CSV;
						];
	rr:subjectMap [
		rr:template "http://research.tib.eu/clarify2020/entity/{organizationID}";
		rr:class clarify:Hospital
	];
	rr:predicateObjectMap [
		rr:predicate owl:sameAs;
		rr:objectMap [
			rr:template "http://research.tib.eu/clarify2020/entity/{autonomous_community}";		
		]
	].