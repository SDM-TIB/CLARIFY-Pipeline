<file:///mnt/e/CLARIFY-KG-pipeline/mapping/lc_clinical_notes_SLCG/lives_with_smokers_lc_clinical_notes_SLCG_smoker_information>
    a rr:TriplesMap;
    rml:logicalSource [ rml:source "/data/lc_clinical_notes_SLCG/smoker_information_lc_clinical_notes_SLCG_PROJECT1.csv";
                rml:referenceFormulation ql:CSV
                ];
    rr:subjectMap [
        rml:reference "findSemantic_ub27bL179C24";
        rr:termType rr:IRI
    ];
    rr:predicateObjectMap [
        rr:predicate rdf:type;
        rr:objectMap [
        rr:template  "http://research.tib.eu/clarify2020/vocab/SmokeEnvironment"
        ]
    ];
    rr:predicateObjectMap [
        rr:predicate clarify:hasOriginalValue;
        rr:objectMap [
        rml:reference "lives_with_smokers"
        ]
    ].

